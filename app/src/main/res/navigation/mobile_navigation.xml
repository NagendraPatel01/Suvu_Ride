<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.suviridedriver.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_earning"
        android:name="com.suviridedriver.ui.earning.EarningFragment"
        android:label="Earning"
        tools:layout="@layout/fragment_earning" />

    <fragment
        android:id="@+id/navigation_rides"
        android:name="com.suviridedriver.ui.rides.RideFragment"
        android:label="Rides"
        tools:layout="@layout/fragment_rides" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.suviridedriver.ui.account.AccountFragment"
        android:label="Account"
        tools:layout="@layout/fragment_account" />

</navigation>